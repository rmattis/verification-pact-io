{
  "match": true,
  "comment": "Requests match with regex",
  "expected" : {
    "headers": {"Content-Type": "application/json"},
    "matchingRules": {
      "body": {
        "$.alligator.name": {
          "matchers": [
            {
              "match": "regex",
              "regex": "\\w+"
            }
          ]
        }
      }
